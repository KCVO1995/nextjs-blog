{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npx babel -w ./src --out-dir dist --extensions .ts,.tsx\" ",
    "build": "next build",
    "start": "next start",
    "serve": "node dist/index.js",
    "typeorm:build": "npx babel -w ./src --out-dir dist --extensions \".ts,.tsx\"",

    "migration:revert": "typeorm migration:revert",
    "entity:create": "typeorm entity:create"
  },
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "axios": "^0.21.0",
    "gray-matter": "^4.0.2",
    "next": "^10.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "ua-parser-js": "^0.7.22"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@types/axios": "^0.14.0",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/ua-parser-js": "^0.7.33",
    "concurrently": "^5.3.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.2"
  }
